{
  "name": "authority-filter",
  "version": "0.0.1",
  "description": "管理端用户前端权限过滤工具",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "files": [
    "src/*.js",
    "dist/*.js"
  ],
  "author": "Sherry",
  "license": "MIT",
  "repository": "https://git.code.tencent.com/sherry_npm/authority-filter.git",
